    "homepage": "https://github.com/EdnaSecurity/security-php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jefferyhus",
            "email": "jefferytutorials@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "^6.2",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": {"Edna\\" : "src/Edna"}
    },
    "autoload-dev": {
        "psr-4": {"Tests\\" : "tests/Edna"}
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
